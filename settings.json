          }
        ]
      }
    ],
    "SessionStart": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "bd prime"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "AskUserQuestion",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/scripts/notify-attention.sh 'Claude has a question for you' 2>/dev/null || true"
          }
        ]
      }
    ],
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/scripts/notify-attention.sh 'Claude has finished working' 2>/dev/null || true"
          }
        ]
      }
    ],
    "Notification": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/scripts/notify-attention.sh 'Claude needs your approval to continue' 2>/dev/null || true"
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "input=$(cat); model_name=$(echo \"$input\" | jq -r '.model.display_name'); current_dir=$(echo \"$input\" | jq -r '.workspace.current_dir'); if [ ! -z \"${GITHUB_USER:-}\" ]; then user_display=\"@${GITHUB_USER}\"; else user_display=\"$(whoami)\"; fi; short_dir=$(basename \"$current_dir\"); git_info=\"\"; if [ -d \"$current_dir/.git\" ] || git -C \"$current_dir\" rev-parse --git-dir >/dev/null 2>&1; then branch=$(git -C \"$current_dir\" symbolic-ref --short HEAD 2>/dev/null || git -C \"$current_dir\" rev-parse --short HEAD 2>/dev/null); if [ -n \"$branch\" ]; then if git -C \"$current_dir\" ls-files --error-unmatch -m --directory --no-empty-directory -o --exclude-standard \":/*\" >/dev/null 2>&1; then git_info=\"($branch ✗) \"; else git_info=\"($branch) \"; fi; fi; fi; printf \"%s ➜ %s %s%s\" \"$user_display\" \"$short_dir\" \"$git_info\" \"$model_name\""
  },
